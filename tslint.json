{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "interface-name": false,
    "no-namespace": false,
    "max-classes-per-file": false,
    "no-reference": false
  }
}
